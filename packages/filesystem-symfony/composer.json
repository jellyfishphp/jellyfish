{
  "name": "jellyfish/filesystem-symfony",
  "description": "Jellyfish filesystem symfony bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "jellyfish/filesystem": "dev-master",
    "jellyfish/pimple": "dev-master",
    "symfony/filesystem": "~4.2.2"
  },
  "require-dev": {
    "codeception/codeception": "~2.4.5",
    "mikey179/vfsStream": "~1.6.5",
    "php-coveralls/php-coveralls": "~2.1.0",
    "phpmd/phpmd": "~2.6.0",
    "phpro/grumphp": "~0.14.1",
    "phpstan/phpstan": "~0.9.2",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "~4.0.0",
    "squizlabs/php_codesniffer": "~3.3.1"
  },
  "autoload": {
    "psr-4": {
      "Jellyfish\\FinderSymfony\\": "src/Jellyfish/FilesystemSymfony"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jellyfish\\FinderSymfony\\": "tests/Jellyfish/FilesystemSymfony"
    }
  }
}