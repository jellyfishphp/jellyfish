  "require": {
    "jellyfish/config": "^1.0.0",
    "jellyfish/http": "^1.0.0",
    "jellyfish/pimple": "^1.0.0",
    "laminas/laminas-diactoros": "^2.0.0",
    "psr/http-message": "^1.0.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
