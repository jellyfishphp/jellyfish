{
  "name": "jellyfish/jellyfish",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=8.2",

    "ext-json": "*",

    "dragonmantank/cron-expression": "^3.0.0",
    "guzzlehttp/guzzle": "^7.0.0",
    "laminas/laminas-diactoros": "^2.0.0",
    "laminas/laminas-httphandlerrunner": "^1.0.0 || ^2.0.0",
    "league/route": "^4.0.0",
    "monolog/monolog": "^3.0.0",
    "opis/json-schema": "^2.0.0",
    "php-amqplib/php-amqplib": "^3.0.0",
    "phpdocumentor/reflection-docblock": "^5.0.0",
    "pimple/pimple": "^3.2.3",
    "predis/predis": "^1.0.0",
    "ramsey/uuid": "^4.0.0",
    "symfony/cache": "^7.0.0",
    "symfony/console": "^7.0.0",
    "symfony/filesystem": "^7.0.0",
    "symfony/lock": "^7.0.0",
    "symfony/process": "^7.0.0",
    "symfony/property-access": "^7.0.0",
    "symfony/property-info": "^7.0.0",
    "symfony/serializer": "^7.0.0",
    "symfony/finder": "^7.0.0",
    "twig/twig": "^3.0.0"
  },
  "require-dev": {
    "codeception/codeception": "*",
    "mikey179/vfsstream": "*",
    "phpstan/phpstan": "*",
    "phpstan/phpstan-phpunit": "*",
    "squizlabs/php_codesniffer": "*",
    "dg/bypass-finals": "*",
    "rector/rector": "*",
    "slevomat/coding-standard": "*"
  },
  "bin": [
    "packages/application/bin/console"
  ],
  "scripts": {
    "test": "codecept run",
    "phpstan": "phpstan analyse --memory-limit 1G -c phpstan.neon packages",
    "phpcs": "phpcs --standard=ruleset.xml --extensions=php packages/*/src/ packages/*/tests/",
    "phpcbf": "phpcbf --standard=ruleset.xml --extensions=php packages/*/src/ packages/*/tests/"
  },
  "autoload": {
    "psr-4": {
      "Jellyfish\\Application\\": "packages/application/src/Jellyfish/Application",
      "Jellyfish\\Cache\\": "packages/cache/src/Jellyfish/Cache",
      "Jellyfish\\CacheSymfony\\": "packages/cache-symfony/src/Jellyfish/CacheSymfony",
      "Jellyfish\\Config\\": "packages/config/src/Jellyfish/Config",
      "Jellyfish\\Event\\": "packages/event/src/Jellyfish/Event",
      "Jellyfish\\EventCache\\": "packages/event-cache/src/Jellyfish/EventCache",
      "Jellyfish\\EventLog\\": "packages/event-log/src/Jellyfish/EventLog",
      "Jellyfish\\Feed\\": "packages/feed/src/Jellyfish/Feed",
      "Jellyfish\\Filesystem\\": "packages/filesystem/src/Jellyfish/Filesystem",
      "Jellyfish\\FilesystemSymfony\\": "packages/filesystem-symfony/src/Jellyfish/FilesystemSymfony",
      "Jellyfish\\Finder\\": "packages/finder/src/Jellyfish/Finder",
      "Jellyfish\\FinderSymfony\\": "packages/finder-symfony/src/Jellyfish/FinderSymfony",
      "Jellyfish\\Http\\": "packages/http/src/Jellyfish/Http",
      "Jellyfish\\JsonSchema\\": "packages/json-schema/src/Jellyfish/JsonSchema",
      "Jellyfish\\JsonSchemaOpis\\": "packages/json-schema-opis/src/Jellyfish/JsonSchemaOpis",
      "Jellyfish\\Kernel\\": "packages/kernel/src/Jellyfish/Kernel",
      "Jellyfish\\Lock\\": "packages/lock/src/Jellyfish/Lock",
      "Jellyfish\\LockSymfony\\": "packages/lock-symfony/src/Jellyfish/LockSymfony",
      "Jellyfish\\Log\\": "packages/log/src/Jellyfish/Log",
      "Jellyfish\\Queue\\": "packages/queue/src/Jellyfish/Queue",
      "Jellyfish\\QueueRabbitMq\\": "packages/queue-rabbit-mq/src/Jellyfish/QueueRabbitMq",
      "Jellyfish\\Process\\": "packages/process/src/Jellyfish/Process",
      "Jellyfish\\ProcessSymfony\\": "packages/process-symfony/src/Jellyfish/ProcessSymfony",
      "Jellyfish\\Redis\\": "packages/redis/src/Jellyfish/Redis",
      "Jellyfish\\Scheduler\\": "packages/scheduler/src/Jellyfish/Scheduler",
      "Jellyfish\\Serializer\\": "packages/serializer/src/Jellyfish/Serializer",
      "Jellyfish\\SerializerSymfony\\": "packages/serializer-symfony/src/Jellyfish/SerializerSymfony",
      "Jellyfish\\Transfer\\": "packages/transfer/src/Jellyfish/Transfer",
      "Jellyfish\\Uuid\\": "packages/uuid/src/Jellyfish/Uuid",
      "Jellyfish\\UuidRamsey\\": "packages/uuid-ramsey/src/Jellyfish/UuidRamsey"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jellyfish\\Application\\": "packages/application/tests/Jellyfish/Application",
      "Jellyfish\\Cache\\": "packages/cache/tests/Jellyfish/Cache",
      "Jellyfish\\CacheSymfony\\": "packages/cache-symfony/tests/Jellyfish/CacheSymfony",
      "Jellyfish\\Config\\": "packages/config/tests/Jellyfish/Config",
      "Jellyfish\\Event\\": "packages/event/tests/Jellyfish/Event",
      "Jellyfish\\EventCache\\": "packages/event-cache/tests/Jellyfish/EventCache",
      "Jellyfish\\EventLog\\": "packages/event-log/tests/Jellyfish/EventLog",
      "Jellyfish\\Feed\\": "packages/feed/tests/Jellyfish/Feed",
      "Jellyfish\\Filesystem\\": "packages/filesystem/tests/Jellyfish/Filesystem",
      "Jellyfish\\FilesystemSymfony\\": "packages/filesystem-symfony/tests/Jellyfish/FilesystemSymfony",
      "Jellyfish\\Finder\\": "packages/finder/tests/Jellyfish/Finder",
      "Jellyfish\\FinderSymfony\\": "packages/finder-symfony/tests/Jellyfish/FinderSymfony",
      "Jellyfish\\Http\\": "packages/http/tests/Jellyfish/Http",
      "Jellyfish\\JsonSchemaOpis\\": "packages/json-schema-opis/tests/Jellyfish/JsonSchemaOpis",
      "Jellyfish\\Kernel\\": "packages/kernel/tests/Jellyfish/Kernel",
      "Jellyfish\\Log\\": "packages/log/tests/Jellyfish/Log",
      "Jellyfish\\Lock\\": "packages/lock/tests/Jellyfish/Lock",
      "Jellyfish\\LockSymfony\\": "packages/lock-symfony/tests/Jellyfish/LockSymfony",
      "Jellyfish\\Queue\\": "packages/queue/tests/Jellyfish/Queue",
      "Jellyfish\\QueueRabbitMq\\": "packages/queue-rabbit-mq/tests/Jellyfish/QueueRabbitMq",
      "Jellyfish\\ProcessSymfony\\": "packages/process-symfony/tests/Jellyfish/ProcessSymfony",
      "Jellyfish\\Redis\\": "packages/redis/tests/Jellyfish/Redis",
      "Jellyfish\\Scheduler\\": "packages/scheduler/tests/Jellyfish/Scheduler",
      "Jellyfish\\SerializerSymfony\\": "packages/serializer-symfony/tests/Jellyfish/SerializerSymfony",
      "Jellyfish\\Transfer\\": "packages/transfer/tests/Jellyfish/Transfer",
      "Jellyfish\\Uuid\\": "packages/uuid/tests/Jellyfish/Uuid",
      "Jellyfish\\UuidRamsey\\": "packages/uuid-ramsey/tests/Jellyfish/UuidRamsey"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
